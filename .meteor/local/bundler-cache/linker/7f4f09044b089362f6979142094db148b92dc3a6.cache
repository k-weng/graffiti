ty\",1);                                  //\n      // debugger                                                      //\n      d3.select(\"#node-\" + old._id).transition().duration(10).style(\"opacity\", function (d) {\n        console.log(\"life \" + d.life / 1000);                          // 86\n        console.log(\"new opacity \" + d.life / maxLife);                // 87\n        return d.life / maxLife;                                       // 88\n      }).transition().duration(function (d) {                          //\n        console.log(d.life, maxLife);return d.life;                    // 90\n      }).style(\"opacity\", 0).each('end', function (d) {                //\n        div.transition().duration(500).style(\"opacity\", 0);            // 93\n        removeNode(d);                                                 // 96\n      });                                                              //\n                                                                       //\n      // .html(\"<small>\" + new Date(d.timestamp) + \"<div> Clicks: \" + d.votes + \" </div>\" + \"</small>\"  + \" <hr>\" +  \"<div> \\\"\" + d.text + \"\\\"</div>\" + \"<br><div>   -\" + d.username + \"</div>\") \n      //           .style(\"font-family\",\"Merriweather\")                //\n      //           .style(\"font-size\",\"11pt\");                         //\n                                                                       //\n      div.style(\"background-color\", function () {                      // 103\n        var username = Meteor.user().username;                         // 104\n        if (Messages.find({ _id: old._id, voters: username }).count() === 0) {\n          return \"#95a5a6\";                                            // 106\n        } else {                                                       //\n          return \"#e74c3c\";                                            // 109\n        }                                                              //\n  