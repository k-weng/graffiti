h).attr(\"id\", \"svg\").attr(\"pointer-events\", \"all\").attr(\"viewBox\", \"0 0 \" + w + \" \" + h).attr(\"perserveAspectRatio\", \"xMinYMid\");\n                                                                       //\n    var vis = svg.append('svg:g');                                     // 140\n                                                                       //\n    var force = d3.layout.force();                                     // 142\n                                                                       //\n    var nodes = force.nodes();                                         // 144\n                                                                       //\n    // var label = d3.select(\"#bubble-labels\");                        //\n                                                                       //\n    var update = function () {                                         // 149\n                                                                       //\n      var node = vis.selectAll(\"g.node\").data(nodes, function (d) {    // 151\n        return d._id;                                                  // 152\n      });                                                              //\n                                                                       //\n      // var labels = d3.selectAll(\".bubble-label\")                    //\n      //   .data(nodes, function(d) { return d._id; });                //\n                                                                       //\n      var nodeEnter = node.enter().append(\"g\").attr(\"class\", \"node\").call(force.drag);\n                                                                       //\n      nodeEnter.append(\"svg:circle\").attr(\"r\", function (d) {          // 161\n        return d.radius;                                               // 162\n      }).attr(\"id\", function (d) {                                     //\n        return \"Node;\" + d.id;                               